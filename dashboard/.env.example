# Dashboard Environment Configuration
# Copy this file to .env and adjust values as needed

# Flask Configuration
FLASK_ENV=production
FLASK_DEBUG=0

# Server Configuration
HOST=0.0.0.0
PORT=5000

# Data Directory (relative to project root)
DATA_DIR=../data

# CORS Origins (comma-separated, * for all)
CORS_ORIGINS=*

# Logging
LOG_LEVEL=INFO
